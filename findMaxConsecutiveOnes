class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int ones = 0;
        int prev = 0;
        for(int i =0;i<nums.length;i++){
            if(nums[i]==1){
                ones+=1;
            }else{
               if(ones > prev){
                    prev=ones;
               } 
              
               ones=0;    
                    
            }
        }
       if (prev>ones){
           return prev;
       }else{
           return ones;
       }
    }
}
